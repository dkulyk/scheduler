{
    "name": "dkulyk/scheduler",
    "description": "Scheduler for laravel jobs",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Kulyk",
            "email": "dmytro@dkulyk.name"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "awesome-nova/tool-actions": "^0.1.0 | ^0.2.0",
        "illuminate/database": ">=6.0",
        "illuminate/support": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "DKulyk\\Scheduler\\": "src"
        },
        "classmap": [
            "database/migrations"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "module": {
            "providers": [
                "\\DKulyk\\Scheduler\\ModuleProvider"
            ]
        }
    }
}
